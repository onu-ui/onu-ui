{
  "name": "onu-ui",
  "type": "module",
  "version": "1.0.8",
  "description": "Onu-UI for web glassmorphism components generate by UnoCSS.",
  "keywords": [
    "onu-ui",
    "unocss",
    "vueuse"
  ],
  "license": "MIT",
  "author": "chris-zhu <https://github.com/chris-zhu>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onu-ui/onu-ui.git",
    "directory": "packages/onu-ui"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/onu-ui/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/onu-ui/src/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "volar.d.ts"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "vue-tsc --declaration --emitDeclarationOnly --skipLibCheck && tsup",
    "stub": "vue-tsc --declaration --emitDeclarationOnly --skipLibCheck && tsup --watch"
  },
  "bugs": {
    "url": "https://github.com/onu-ui/onu-ui/issues"
  },
  "homepage": "https://github.com/onu-ui/onu-ui#readme",
  "dependencies": {
    "@onu-ui/components": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
