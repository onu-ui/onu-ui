{
  "name": "@onu-ui/components",
  "type": "module",
  "version": "1.0.7",
  "description": "Onu-UI components for use on demand",
  "main": "./dist/components.umd.cjs",
  "module": "./dist/components.js",
  "exports": {
    ".": {
      "import": "./dist/components.js",
