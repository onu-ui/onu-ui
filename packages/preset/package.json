{
  "name": "@onu-ui/preset",
  "type": "module",
  "version": "1.1.2",
  "description": "Opinionated Onu UI preset for UnoCSS.",
  "author": "Chris <https://github.com/zyyv>",
  "license": "MIT",
  "homepage": "https://github.com/onu-ui/onu-ui/blob/main/packages/preset/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onu-ui/onu-ui.git",
    "directory": "packages/preset"
  },
  "bugs": {
    "url": "https://github.com/onu-ui/onu-ui/issues"
  },
  "keywords": [
    "onu-ui",
    "@onu-ui/preset",
    "unocss preset"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./helper": {
      "types": "./dist/helper.d.ts",
      "import": "./dist/helper.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@unocss/preset-mini": "catalog:",
    "magic-color": "catalog:"
  },
  "devDependencies": {
    "unocss": "catalog:"
  }
}
